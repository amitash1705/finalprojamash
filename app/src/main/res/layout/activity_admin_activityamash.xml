<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminActivityamash">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="66dp"
        android:fontFamily="@font/mimosa"
        android:text="Admin Page"
        android:textColor="@color/black"
        android:textSize="50dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btnAddNewAttraction"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnAddNewAttraction"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="59dp"
        android:backgroundTint="#322196F3"
        android:fontFamily="@font/mimosa"
        android:text="Add Attraction"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toTopOf="@+id/btnpro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/btnpro"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="62dp"
        android:backgroundTint="#322196F3"
        android:fontFamily="@font/mimosa"
        android:text="Profile"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toTopOf="@+id/btnattraction"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAddNewAttraction" />

    <Button
        android:id="@+id/btnattraction"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="59dp"
        android:backgroundTint="#322196F3"
        android:fontFamily="@font/mimosa"
        android:text="Attraction"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toTopOf="@+id/btntrip"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnpro" />

    <Button
        android:id="@+id/btntrip"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:backgroundTint="#322196F3"
        android:fontFamily="@font/mimosa"
        android:text="Trips"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toTopOf="@+id/btnBackHome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnattraction" />

    <Button
        android:id="@+id/btnBackHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:backgroundTint="#322196F3"
        android:fontFamily="@font/mimosa"
        android:text="back"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:cornerRadius="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btntrip" />

</androidx.constraintlayout.widget.ConstraintLayout>